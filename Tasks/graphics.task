ScheduledTask[
    Module[{chart},
        Get[FileNameJoin[{"`root`", "Packages", "Graphics", "`type`.wl"}]];
        chart = `func`["`config`", Today];
        If[Head[chart] =!= Graphcis, Return[$Failed]];
        CloudExport[chart, "PNG", "`dest`"]
    ],
    `schedule`,
    NotificationFunction -> `admin`
]